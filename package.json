{
  "name": "Vue-Boilerplate",
  "version": "1.0.0",
  "description": "Minimalist Vue Boilerplate",
  "main": "main.js",
  "scripts": {
    "dev": "webpack --mode development --watch ./src/index.js --output ./dist/vue.dev.compiled.js",
    "build": "webpack --mode production ./src/index.js --output ./dist/vue.compiled.js",
    "build::min": "webpack --mode production ./src/index.js --output ./dist/vue.compiled.min.js",
    "start": "webpack-dev-server --host 0.0.0.0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "bootstrap": "^4.4.1",
    "es6-promise": "^4.2.8",
    "fs": "0.0.1-security",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuetify": "^2.2.21",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^3.4.2",
    "node-sass": "^7.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  }
}
